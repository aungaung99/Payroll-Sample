<!-- Content Header (Page header) -->
<div class="content-header">
	<div class="container-fluid">
		<div class="row mb-2">
			<div class="col-sm-6">
				@if (ViewBag.SubPageTitle == null)
				{
					<h1 class="m-0">@ViewBag.PageTitle</h1>
				}
				else
				{
					<h1 class="m-0">@ViewBag.SubPageTitle</h1>
				}
			</div><!-- /.col -->
			<div class="col-sm-6">
				<ol class="breadcrumb float-sm-right">
					<li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
					@if (ViewBag.SubPageTitle != null)
					{
						<li class="breadcrumb-item">@ViewBag.PageTitle</li>
					}
					else
					{
						<li class="breadcrumb-item active">@ViewBag.PageTitle</li>
					}
					@if (ViewBag.SubPageTitle != null)
					{
						<li class="breadcrumb-item active">@ViewBag.SubPageTitle</li>
					}
				</ol>
			</div><!-- /.col -->
		</div><!-- /.row -->
	</div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->